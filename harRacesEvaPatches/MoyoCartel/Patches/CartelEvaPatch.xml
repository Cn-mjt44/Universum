            <xpath>Defs/ThingDef[defName="Moyo_CEsuit" or defName="Moyo_CEhelmet"]/apparel/tags</xpath>
            <nomatch Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[defName="Moyo_CEsuit" or defName="Moyo_CEhelmet"]/apparel</xpath>
                <value>
                    <tags />
                </value>
            </nomatch>
        </Operation>
